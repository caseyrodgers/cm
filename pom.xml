   						     <module>hotmath.gwt.cm_search.CatchupMathSearch</module>
						     <module>hotmath.gwt.cm_tutor.CmTutor</module>

                                                     <module>hotmath.gwt.cm_activity_word_problems.CmActivityWordProblems</module>


						     <runTarget>hotmath.gwt.cm.CatchupMath/CatchupMath.html</runTarget> 
                                                -->
					</modules>
					<hostedWebapp>
						${project.build.directory}/${project.build.finalName}
					</hostedWebapp>
				</configuration>
				<executions>
					<execution>
						<goals>
							<goal>compile</goal>
						</goals>
					</execution>
				</executions>

				<!-- added here to override default allow use of 2.2 version of gwt-maven 
					plugin. Once new 2.3 version is released, this will no longer be required. -->

				<dependencies>
					<dependency>
						<groupId>com.google.gwt</groupId>
						<artifactId>gwt-user</artifactId>
						<version>2.5.0</version>
					</dependency>
					<dependency>
						<groupId>com.google.gwt</groupId>
						<artifactId>gwt-dev</artifactId>
						<version>2.5.0</version>
					</dependency>
					<dependency>
						<groupId>com.google.gwt</groupId>
						<artifactId>gwt-servlet</artifactId>
						<version>2.5.0</version>
					</dependency>
				</dependencies>
			</plugin>


