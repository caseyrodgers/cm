<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
     xmlns:g="urn:import:com.google.gwt.user.client.ui">
     <ui:style>
          .bolder { font-weight:bold; color: green;}
     </ui:style>
     <g:HTMLPanel>
          <g:Button ui:field="checkTest">Check Test</g:Button>
          <g:CheckBox ui:field="checkOffline">Offline</g:CheckBox>
          <g:Label styleName="{style.bolder}" text="Catchup Math Mobile Quiz POC"/>
          <g:VerticalPanel ui:field="mainPanel" spacing="5"/>
     </g:HTMLPanel>
</ui:UiBinder>
