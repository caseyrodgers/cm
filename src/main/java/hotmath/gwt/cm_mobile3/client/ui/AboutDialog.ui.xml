	<ui:style>
		.main {
		    padding: 10px;
		}
		.main .footer {
		    padding-left: 10px;
		    padding-bottom: 5px;
		}
	</ui:style>

	<g:HTMLPanel styleName="{style.main}">
	   <p>You are logged in as: <span ui:field="loggedInAs"></span></p>
