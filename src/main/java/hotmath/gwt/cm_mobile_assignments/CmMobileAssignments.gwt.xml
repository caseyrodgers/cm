<module rename-to='cm_mobile_assignments'>
      <inherits name="com.google.gwt.user.User" />
      
      <inherits name="hotmath.gwt.cm_core.CmCore"/>
      <inherits name="hotmath.gwt.cm_rpc.CmRpc"/>
      <inherits name="hotmath.gwt.cm_rpc_assignments.CmRpcAssignments"/>
      <inherits name="hotmath.gwt.cm_tutor.CmTutor"/>
            
      <entry-point class='hotmath.gwt.cm_mobile_assignments.client.CmMobileAssignments'/>
      
      <inherits name="com.googlecode.mgwt.MGWT"/>
      
      <!--
      <inherits name="hotmath.gwt.cm_mobile_shared.CatchupMathMobileShared"/> 
      <set-property name="user.agent" value="safari" />
       -->
      
	<!-- Use ClientFactoryImpl by default -->
	<replace-with class="hotmath.gwt.cm_mobile_assignments.client.ClientFactoryImplDefault">
		<when-type-is class="hotmath.gwt.cm_mobile_assignments.client.ClientFactory" />
	</replace-with>
      
</module>
